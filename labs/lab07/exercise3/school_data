import school_data

print("=== School Data ===")
print(f"Student Name: {school_data.name}")
print(f"Student ID: {school_data.student_id}")
print(f"age: {school_data.age}")
print(f"Course Code: {school_data.course_code}")
print(f"Course Name: {school_data.course_name}")

print(f"Score 1: {school_data.score1}")
print(f"Score 2: {school_data.score2}")
print(f"Total Score: {school_data.total_score}")
print(f"Total Score (sqrt): {school_data.total_score1}") 

print(f"Name in UPPERCASE: {school_data.name.upper()}")
print(f"Name in lowercase: {school_data.name.lower()}")
print(f"Name Length: {school_data.name_length} characters")


